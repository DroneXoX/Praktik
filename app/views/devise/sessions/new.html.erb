<div class="card">
  <h5 class="card-header">Logga in</h5>
  <div class="card-body"><%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field form-group">
    <% f.label :email %><br />
    <%= f.email_field :email, class: "form-control", placeholder:"Mailadress", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field form-group">
    <% f.label :password %><br />
    <%= f.password_field :password, class: "form-control", placeholder: "Lösenord", autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :Kom_ihåg %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Logga in", class: 'btn btn-primary' %>
  </div>
<% end %>
  </div>
</div>


<br>
<%= render "devise/shared/links" %>
